<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="400"
			 xmlns:c="clr-namespace:RoundButton.Controls;assembly=RoundButton"
             x:Class="RoundButton.ViewModels.TestUserControl">
	<StackPanel HorizontalAlignment="Center">
		<Button
			Classes="TestBtn"
			VerticalAlignment="Center"
			HorizontalAlignment="Left"
			Height="40"
			Width="40">
		</Button>

		<c:CircularActionButton
						Height="40"
						Width="40"
						Margin="7"
						PathData="{DynamicResource ExitBtnGeometry}"
						HorizontalAlignment="Left"
						ToolTip.Tip="Close">
		</c:CircularActionButton>

		<c:CircularActionButton
							Height="40"
							Width="40"
							Margin="7"
							IsLoading="True"
							PathData="{DynamicResource ExitBtnGeometry}"
							HorizontalAlignment="Left"
							ToolTip.Tip="Close"
							Command="{Binding ExitCommand}">
		</c:CircularActionButton>

	</StackPanel>
</UserControl>
